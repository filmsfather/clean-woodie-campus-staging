{
  "version": 2,
  "installCommand": "cd ../.. && pnpm install --frozen-lockfile",
  "buildCommand": "cd ../.. && pnpm run build",
  "outputDirectory": "packages/api/dist",
  "builds": [
    {
      "src": "packages/api/dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/packages/api/dist/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
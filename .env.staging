# Staging Environment Configuration
# Do not put sensitive keys here - use GitHub Secrets instead

VITE_APP_NAME="Clean Woodie Campus (Staging)"
VITE_APP_ENV=staging
VITE_APP_VERSION=$npm_package_version
VITE_API_BASE_URL=https://staging-api.clean-woodie-campus.com

# Supabase configuration (injected from GitHub Secrets)
# VITE_SUPABASE_URL (from secrets.STAGING_SUPABASE_URL)
# VITE_SUPABASE_ANON_KEY (from secrets.STAGING_SUPABASE_ANON_KEY)

# Feature flags - 새로 구현된 UseCase UI 표면들
VITE_ENABLE_DEV_TOOLS=true
VITE_ENABLE_ANALYTICS=false

# Auth System Features (완전 구현됨)
VITE_FEATURE_SIGNIN=true
VITE_FEATURE_SIGNUP=true
VITE_FEATURE_PROFILE=true

# SRS System Features (완전 구현됨)
VITE_FEATURE_REVIEW_SYSTEM=true
VITE_FEATURE_REVIEW_STATISTICS=true

# Progress Management Features (완전 구현됨)  
VITE_FEATURE_CLASS_PROGRESS=true
VITE_FEATURE_STREAK_RANKINGS=true

# Gamification Features (완전 구현됨)
VITE_FEATURE_LEADERBOARDS=true
VITE_FEATURE_REWARDS=true
VITE_FEATURE_TOKENS=true

# MSW API Mocking (스테이징에서는 비활성화)
VITE_MSW_ENABLED=false

# Storybook (스테이징에서도 접근 가능)
VITE_STORYBOOK_ENABLED=true